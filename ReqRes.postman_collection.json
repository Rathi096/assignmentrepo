			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//Validate response status\r",
							"pm.test(\"Response status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    console.log(\"Response status code is 200\");\r",
							"});\r",
							"\r",
							"//Validate payload response structure\r",
							"\r",
							" pm.test(\"Response should have expected payload structure\", () => {\r",
							"    const res = pm.response.json(); \r",
							"    pm.expect(res).to.have.property(\"id\");\r",
							"    pm.expect(res).to.have.property(\"token\");\r",
							"    console.log(\"Response payload structure is valid\");\r",
							"});\r",
							"\r",
							"\r",
							"// Validate business logic\r",
							"pm.test(\"Valid ID and token structure\", function () {\r",
							"    const responseJSON = pm.response.json();\r",
							"    pm.expect(responseJSON.id).to.be.a('number').and.to.be.greaterThan(0);\r",
							"    pm.expect(responseJSON.token).to.be.a('string').and.to.have.length.above(15);\r",
							"    console.log(\"Response business logic is valid\")\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
